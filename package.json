{
  "name": "sanity-web-react-components",
  "version": "1.0.0-alpha.23",
  "description": "Shared React components for web apps on Sanity.io.",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "repository": "https://github.com/sanity-io/sanity-web-react-components",
  "homepage": "https://github.com/sanity-io/sanity-web-react-components#readme",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-web-react-components/issues"
  },
  "keywords": [
    "sanity.io",
    "web",
    "react",
    "components",
    "css modules"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "scripts": {
    "build": "run-s clean tsc-cjs tsc-es postcss-cjs postcss-es",
    "clean": "rimraf lib",
    "dev": "run-p watch-*",
    "postcss-cjs": "mkdirp lib/cjs && postcss src/**/*.css --base src --dir lib/cjs/",
    "postcss-es": "mkdirp lib/es && postcss src/**/*.css --base src --dir lib/es/",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc-cjs": "tsc -p ts/tsconfig.cjs.json",
    "tsc-es": "tsc -p ts/tsconfig.es.json",
    "watch-dev-server": "node dev/start.js",
    "watch-postcss-cjs": "mkdirp lib/cjs && postcss -w src/**/*.css --base src --dir lib/cjs/",
    "watch-postcss-es": "mkdirp lib/es && postcss -w src/**/*.css --base src --dir lib/es/",
    "watch-tsc-cjs": "tsc -w -p ts/tsconfig.cjs.json",
    "watch-tsc-es": "tsc -w -p ts/tsconfig.es.json"
  },
  "devDependencies": {
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "css-modules-require-hook": "^4.2.3",
    "express": "^4.17.1",
    "highlight.js": "^9.15.8",
    "mkdirp": "^0.5.1",
    "module-alias": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-lowlight": "^2.0.0",
    "react-props-stream": "^1.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "sanity-web-styles": "^1.0.0-alpha.13",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.2",
    "webpack": "^4.34.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "peerDependencies": {
    "highlight.js": "^9.15.8",
    "react": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-lowlight": "^2.0.0",
    "sanity-web-styles": "^1.0.0-alpha.13"
  },
  "_moduleAliases": {
    "@dev": "src/@dev"
  }
}
